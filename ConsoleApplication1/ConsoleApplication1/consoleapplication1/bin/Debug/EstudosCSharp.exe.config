<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
    </configSections>
    <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
    <connectionStrings>
        <clear />
        <add name="SQLServerLocal" connectionString="Data Source=HELDER\SQLEXPRESS;Initial Catalog=EstudosCSharp;Integrated Security=True;"
            providerName="System.Data.SqlClient" />
        <add name="EstudosCSharp.Properties.Settings.EstudosCSharpConnectionString"
            connectionString="Data Source=HELDER\SQLEXPRESS;Initial Catalog=EstudosCSharp;Integrated Security=True"
            providerName="System.Data.SqlClient" />
    </connectionStrings>

    <!-- Listener to Debug events -->
    <system.diagnostics>
      <trace autoflush="true" indentsize="4">
        <listeners>
          <add name="myTexWriterTraceListener"
          type="System.Diagnostics.TextWriterTraceListener"
          initializeData="TextWriterTraceListenerOutput.log" />
          <!--<remove name="Default" />-->
        </listeners>
      </trace>
    </system.diagnostics>
</configuration>
